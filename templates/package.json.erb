{
  "name": "hosted-hubot",
  "version": "<%= @hubotversion -%>",
  "private": true,
  "author": "GitHub Inc.",

  "keywords": [
    "github",
    "hubot",
    "campfire",
    "bot"
  ],

  "description": "A simple helpful robot for your Company",

  "dependencies": {
    <%= @dependencies.map{|key, value| %Q("#{key}": "#{value}")}.join(",\n    ") %>
  },

  "engines": {
    "node": ">= 0.8.x",
    "npm":  ">= 1.1.x"
  },

  "main": "./index",

  "bin": {
    "hubot": "./bin/hubot"
  }
}
